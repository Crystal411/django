function showMen(n){
	for(var i=1;i<=2;i++){
		var curCon=document.getElementById("pic_"+i);
		var curBtn=document.getElementById("picb_"+i);
		if(n==i){
			curBtn.style.display="block";
			curCon.className="one"
		}else{
			curBtn.style.display="none";
			curCon.className="";			
		}
	}
}
function showNew(n){
	for(var i=1;i<=7;i++){
		var curBtn=document.getElementById("new_"+i);
		var curCon=document.getElementById("new_c_"+i);
		if(n==i){
			curCon.style.display="block";
			curBtn.className="one";
		}else{
			curCon.style.display="none";
			curBtn.className="";
		}
	}
}
